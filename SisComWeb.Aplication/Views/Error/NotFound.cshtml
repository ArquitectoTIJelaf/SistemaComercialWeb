@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" type="image/png" sizes="16x16" href="~/favicon.ico">
    <title>Error Page</title>
    <link href="~/wwwroot/statics/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/wwwroot/statics/css/animate.css" rel="stylesheet" />
    <link href="~/wwwroot/statics/css/style.css" rel="stylesheet" />
    <link href="~/wwwroot/statics/css/colors/default.css" rel="stylesheet" />
</head>
<body>

    <section id="wrapper" class="error-page">
        <div class="error-box">
            <div class="error-body text-center">
                <h1 class="text-danger">404</h1>
                <h3 class="text-uppercase">¡Página no encontrada!</h3>
                <p class="text-muted m-t-30 m-b-30">Por favor, intente nuevamente o comuníquese con su administrador</p>
                <a href="@Url.Action("Index","Default")" class="btn btn-danger btn-rounded waves-effect waves-light m-b-40">Regresar</a>
            </div>
            <footer class="footer text-center"><strong>Copyright &copy; @DateTime.Now.Year </strong>&nbsp;&nbsp;&nbsp;&nbsp;JELAF INTEGRADORES S.R.L. - Todos los derechos reservados.</footer>
        </div>
    </section>
    <script src="~/wwwroot/plugins/bower_components/jquery/dist/jquery.min.js"></script>
    <script src="~/wwwroot/statics/bootstrap/dist/js/bootstrap.min.js"></script>

</body>
</html>


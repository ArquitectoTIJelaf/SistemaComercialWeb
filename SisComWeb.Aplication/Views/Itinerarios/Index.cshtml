                appVueTipoPago.tipoPago = '01';
                appVueTipoPago.$nextTick(function () {
                    switch (appVueTipoPago.pickedBusVenta) {
                        case '1':
                            appVueTipoPago.importePagado = appVueTipoPago.sumaTotal;
                            appVueTipoPago.vuelto = '0.00';

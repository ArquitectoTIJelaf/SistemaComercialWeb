                            return;
                        }

                        if (!auxObj.FechaVenta && auxObj.NumeAsiento === 0) {
                            this.auxFocus.asiento = false;
                            var auxResponse = await this.bloquearAsiento(auxObj, true);
                            if (!auxResponse) {

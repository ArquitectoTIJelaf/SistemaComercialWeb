        var appVueFilter = new Vue({
            el: '#appVueFilter',
            data: {
                filtro: {
                    CodiOrigen: null,
                    CodiDestino: selectAll,
                    CodiRuta: selectAll,
                    FechaViaje: fechaActual,
                    Hora: horaActual,

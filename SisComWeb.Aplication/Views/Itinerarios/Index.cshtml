            },
            mounted: function () {
                document.getElementsByClassName("borderBottom")[0].classList.add("active");

                $(".preloader").fadeIn();
                this.getSucursales();
                this.$nextTick(() => {
                    $('#cboDestinoPas input[type=search]').focus();

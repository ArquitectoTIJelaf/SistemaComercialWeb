                    _this.item.numero = correlativo.substring(5, 13);
                    _this.item.empresa = appVueVenta.$refs.refEmpresa.value.label; //TODO: Confirmar
                    _this.item.codEmpresa = appVueVenta.$refs.refEmpresa.value.id; //TODO: Confirmar
                },
                configEventKeyF9: function (event) {
                    if (event.keyCode === 120) {
                        if (appVueVenta.showVentas) {
                            this.show();
                        }                        
                    }
                },
                searchData: async function () {
                    let _this = this;
                    await APP.rq.axios2.post(APP.base + '/itinerarios/buscaBoletoF9' + APP.util.getQueryParamByObject({
